{
  "name": "electron-quasar-typesafe",
  "version": "3.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "derushio",
  "scripts": {
    "test": "vitest --reporter=html --outputFile=_testReports/index.html --run",
    "test_ui": "vitest --ui --coverage",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "prepare:electron": "electron-builder install-app-deps",
    "prepare:backend": "pnpm rebuild -r",
    "start": "electron-vite preview",
    "dev": "pnpm run prepare:electron && electron-vite dev",
    "devs": "run-p dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "pnpm run prepare:backend && tsx src/main/server.ts",
    "drizzle:init": "pnpm run prepare:backend &&  tsx ./src/main/repositories/db/index.ts",
    "drizzle:generate": "pnpm run prepare:backend && tsx ./node_modules/drizzle-kit/bin.cjs generate:sqlite",
    "drizzle:drop": "pnpm run prepare:backend && tsx ./node_modules/drizzle-kit/bin.cjs drop",
    "drizzle:migrate": "pnpm run prepare:backend && tsx ./src/main/repositories/db/utils/migrate.ts",
    "drizzle:seed": "pnpm run prepare:backend && tsx ./src/main/repositories/db/seed/index.ts",
    "// build": "",
    "env:build": "cp .env .env.stash && cp .env.build .env",
    "env:restore": "cp .env.stash .env",
    "prebuild": "pnpm run env:build && rm -rf dist; rm -rf out",
    "build:code": "pnpm run prepare:electron && pnpm run prebuild && pnpm run typecheck && electron-vite build",
    "postinstall": "",
    "package:win": "pnpm run build:code && electron-builder --win --config",
    "package:mac": "pnpm run build:code && electron-builder --mac --config",
    "package:linux": "pnpm run build:code && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@quasar/extras": "^1.16.4",
    "@trpc/client": "^10.29.0",
    "@trpc/server": "^10.29.0",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.6.0",
    "better-sqlite3": "~9",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "electron-trpc": "^0.5.0",
    "lodash": "^4.17.21",
    "p-queue": "^7.4.1",
    "pinia": "^2.1.3",
    "quasar": "^2.12.0",
    "superjson": "^1.12.3",
    "vue": "^3.3.4",
    "vue-query": "^1.26.0",
    "vue-router": "^4.2.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@quasar/app-webpack": "^3.9.2",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.4.0",
    "@quasar/vite-plugin": "^1.3.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.13",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@typescript-eslint/typescript-estree": "^5.59.9",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.4.1",
    "drizzle-kit": "^0.20.4",
    "electron": "^22.3.4",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.21",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.14.1",
    "happy-dom": "^12.10.3",
    "node-gyp": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "tsx": "^4.5.0",
    "typescript": "^5.1.3",
    "vite": "^4.2.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.2.0"
  }
}
