{
  "name": "electron-quasar-typesafe",
  "version": "3.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "derushio",
  "scripts": {
    "test": "vitest --reporter=html --outputFile=_testReports/index.html --run",
    "test_ui": "vitest --ui --coverage",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "yarn prisma:generate && electron-vite dev",
    "devs": "yarn prisma:generate && run-p dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "MODE=BACKEND vite-node src/main/server.ts",
    "// prisma": "",
    "prisma:migration": "prisma migrate dev && yarn prisma:generate",
    "prisma:generate": "prisma generate",
    "prisma:seed": "MODE=BACKEND vite-node prisma/seed/index.ts",
    "prisma:studio": "prisma studio",
    "// build": "",
    "env:build": "cp .env .env.stash && cp .env.build .env",
    "env:restore": "cp .env.stash .env",
    "prebuild": "env:build && rm -rf dist; rm -rf out",
    "build": "yarn prebuild && yarn prisma:generate && yarn typecheck && electron-vite build",
    "postinstall": "yarn prisma:generate && electron-builder install-app-deps",
    "build:win": "yarn build && electron-builder --win --config",
    "build:mac": "yarn build && electron-builder --mac --config",
    "build:linux": "yarn build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@prisma/client": "^4.15.0",
    "@quasar/extras": "^1.16.4",
    "@trpc/client": "^10.29.0",
    "@trpc/server": "^10.29.0",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.6.0",
    "better-sqlite3": "^8.7.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "electron-trpc": "^0.5.0",
    "knex": "^3.0.1",
    "lodash": "^4.17.21",
    "pinia": "^2.1.3",
    "prisma": "^4.15.0",
    "quasar": "^2.12.0",
    "superjson": "^1.12.3",
    "vue": "^3.3.4",
    "vue-query": "^1.26.0",
    "vue-router": "^4.2.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@quasar/app-webpack": "^3.9.2",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.4.0",
    "@quasar/vite-plugin": "^1.3.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/cors": "^2.8.13",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@typescript-eslint/typescript-estree": "^5.59.9",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.4.1",
    "electron": "^22.3.4",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.21",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.14.1",
    "happy-dom": "^12.10.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vite": "^4.2.1",
    "vite-node": "^0.34.3",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.2.0"
  }
}
