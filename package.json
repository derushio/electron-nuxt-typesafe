{
  "name": "electron-quasar-typesafe",
  "version": "3.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "homepage": "https://example.com",
  "main": "./out/main/index.js",
  "author": "derushio",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "// test": "",
    "test:run": "vitest --reporter=html --outputFile=_testReports/index.html --run",
    "test:ui": "vitest --ui --coverage",
    "// typecheck": "",
    "typecheck": "pnpm typecheck:node && pnpm typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "// prepare": "",
    "prepare:electron": "(cd src/renderer/repositories/trpc && cp index.electron.ts.cp index.ts) && electron-builder install-app-deps",
    "prepare:node": "pnpm rebuild -r",
    "prepare:browser": "(cd src/renderer/repositories/trpc && cp index.browser.ts.cp index.ts)",
    "// electron": "",
    "electron:start": "electron-vite preview",
    "electron:dev": "pnpm prepare:electron && electron-vite dev",
    "electron:prebuild": "pnpm env:build && rm -rf dist; rm -rf out",
    "electron:build": "pnpm prepare:electron && pnpm electron:prebuild && pnpm typecheck && electron-vite build",
    "electron:package:win": "pnpm electron:build && electron-builder --win --config && pnpm env:restore",
    "electron:package:mac": "pnpm electron:build && electron-builder --mac --config && pnpm env:restore",
    "electron:package:linux": "pnpm electron:build && electron-builder --linux --config && pnpm env:restore",
    "// client server spa": "",
    "spa:dev": "run-p spa:dev:frontend spa:dev:backend",
    "spa:dev:frontend": "pnpm prepare:browser && vite",
    "spa:dev:backend": "pnpm prepare:node && tsx src/main/server.ts",
    "// client server ssr nuxt": "",
    "ssr:dev": "run-p ssr:dev:frontend ssr:dev:backend",
    "ssr:dev:frontend": "pnpm prepare:browser && nuxt dev",
    "ssr:dev:backend": "pnpm prepare:node && tsx src/main/server.ts",
    "// drizzle": "",
    "drizzle:init": "pnpm prepare:node && tsx ./src/main/repositories/db/index.ts",
    "drizzle:generate": "pnpm drizzle:init && tsx ./node_modules/drizzle-kit/bin.cjs generate:sqlite",
    "drizzle:drop": "pnpm drizzle:init && tsx ./node_modules/drizzle-kit/bin.cjs drop",
    "drizzle:migrate": "pnpm prepare:node && tsx ./src/main/repositories/db/index.ts && tsx ./src/main/repositories/db/utils/migrate.ts",
    "drizzle:seed": "pnpm prepare:node && tsx ./src/main/repositories/db/index.ts && tsx ./src/main/repositories/db/utils/migrate.ts && tsx ./src/main/repositories/db/seed/index.ts",
    "drizzle:studio": "pnpm drizzle:init && tsx ./node_modules/drizzle-kit/bin.cjs studio",
    "// env": "",
    "env:build": "cp .env .env.stash && cp .env.build .env",
    "env:restore": "cp .env.stash .env",
    "postinstall": ""
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@pinia/nuxt": "^0.5.1",
    "@quasar/extras": "^1.16.4",
    "@trpc/client": "^10.29.0",
    "@trpc/server": "^10.29.0",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.6.0",
    "better-sqlite3": "~9",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "electron-trpc": "^0.5.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "nuxt-quasar-ui": "^2.0.6",
    "p-queue": "^7.4.1",
    "pinia": "^2.1.3",
    "quasar": "^2.12.0",
    "superjson": "^1.12.3",
    "vue": "^3.3.4",
    "vue-query": "^1.26.0",
    "vue-router": "^4.2.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@nuxt/devtools": "^1.0.4",
    "@quasar/app-webpack": "^3.9.2",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.4.0",
    "@quasar/vite-plugin": "^1.3.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.13",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@typescript-eslint/typescript-estree": "^5.59.9",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.4.1",
    "drizzle-kit": "^0.20.4",
    "electron": "^22.3.4",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.21",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.14.1",
    "happy-dom": "^12.10.3",
    "node-gyp": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.8.2",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "tsx": "^4.5.0",
    "typescript": "^5.1.3",
    "vite": "^4.2.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.2.0"
  }
}
